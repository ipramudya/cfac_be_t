{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "printWidth": 100,
  "semi": false,
  "bracketSameLine": true,
  "useTabs": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSortSpecifiers": true,
  "importOrder": ["dotenv/config", "^@/(.*)$", "^[./]", "<THIRD_PARTY_MODULES>"],
  "importOrderParserPlugins": ["typescript"]
}
